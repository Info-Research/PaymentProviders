		</file>
		<file>
			<guid>uWebshopPayPalInstaller.ascx</guid>
			<orgPath>/usercontrols/uWebshop</orgPath>
			<orgName>uWebshopPayPalInstaller.ascx</orgName>
		</file>
	</files>
	<control>/usercontrols/uWebshop/uWebshopPayPalInstaller.ascx</control>
